{
  "name": "web-view-tools",
  "version": "2.0.1",
  "description": "淘宝小程序webView 开发需要用到的一些工具",
  "scripts": {
    "commit": "git-cz",
    "build": "rollup -c"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "lib",
    "types"
  ],
  "main": "lib/web-view-tools.js",
  "types": "types/index.d.ts",
  "devDependencies": {
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "commitizen": "^4.2.3",
    "core-js": "^3.10.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.23.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "rollup": "^2.44.0",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noshower/web-view-tools.git"
  },
  "keywords": [],
  "author": "noshower",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/noshower/web-view-tools/issues"
  },
  "homepage": "https://github.com/noshower/web-view-tools#readme"
}
